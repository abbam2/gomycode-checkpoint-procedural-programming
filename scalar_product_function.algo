ALGORITHM scalar_product

PROCEDURE dot_product(ps:integer;v1,v2:array_of_integer[3])
VAR
    i:integer
BEGIN
    read(v1)
    read(v2)
    FOR i FROM 1 TO N  DO
        read(v1[i])
    END_FOR
    FOR i FROM 1 TO N   DO
        read(v2[i])
    END_FOR

    ps:=0
    FOR i FROM 1 TO N   DO
        ps:=ps+v1[i]*v2[i]
    END_FOR
    write("le produit scalaire est:"ps)
END